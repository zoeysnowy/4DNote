  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "dev:usage-report": "node scripts/dev-usage-report.mjs",
    "coverage:manual": "node scripts/run-manual-v8-coverage.mjs",
    "prune-history": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/prune-local-history.ps1 -Days 14",
    "prune-oom-logs": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/prune-oom-logs.ps1 -Days 14",
    "stop": "taskkill /F /IM node.exe /T & taskkill /F /IM electron.exe /T",
    "electron": "cd electron && npm start",
    "electron-dev": "cd electron && npm run electron-dev",
    "electron-pack": "cd electron && npm run pack",
    "ed": "cd electron && npm run electron-dev",
    "e": "cd electron && npm run electron-dev",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "check-timespec": "node scripts/check-timespec.js",
    "install-hooks": "node scripts/install-hooks.js",
    "ssot:check": "node scripts/clean-deprecated-fields.js --dry-run",
    "ssot:fix": "node scripts/clean-deprecated-fields.js --fix",
    "ssot:setup-hooks": "node scripts/setup-git-hooks.js",
    "download-subs": "node scripts/download-subtitles.js",
    "parse-subs": "node scripts/parse-subtitles.js",
    "setup-rag": "node scripts/setup-rag.js",
    "test-rag": "node scripts/test-rag-search.js",
    "rag-simple": "node scripts/simple-rag-test.js",
    "rag-demo": "node scripts/generate-mock-data.js && node scripts/simple-rag-test.js --batch",
    "rag-hunyuan": "node scripts/test-rag-hunyuan.js",
    "rag-setup-hunyuan": "node scripts/setup-rag-hunyuan.js",
    "proxy-start": "cd ai-proxy && npm start"
  },
